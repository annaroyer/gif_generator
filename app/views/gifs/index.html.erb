<section class="jumbotron">
  <h1>ALL THE GIFS</h1>
</section>
<main class='column-container'>
<% @categories.each do |category| %>
  <h2><%= link_to category.name, category_path(category) %></h2>
  <section class="gifs">
    <%= render partial: 'gif', collection: category.gifs %>
  </section>
<% end %>
</main>
